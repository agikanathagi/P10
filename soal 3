#include <iostream>

using namespace std;

int main(){
   int index,i;
   //int isi=0;

   cout<<"Masukkin indexnya : ";
   cin>>index;

   int A[index];

   for(i=0;i<index;i++){
    cout<<"Nilai ke-"<<i<<" = ";
    cin>>A[i];
   }
-------------
   cout<<endl<<"Nilai yang anda masukkin : ";
   cout<<"A["<<index<<"] = ";

    for(i=0;i<index;i++){
    cout<<"["<<A[i]<<"] ";
   }

   cout<<endl;
return 0;
}
